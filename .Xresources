*.color1:  #f13459
*.color9:  #c31e52
 
*.color2:  #a55e8b
*.color10: #884597
 
*.color3:  #fabb5c
*.color11: #d65f4a
 
*.color4:  #2c7dcf
*.color12: #285ab8
 
*.color5:  #faddc5
*.color13: #fa9584
 
*.color6:  #fa436e
*.color14: #a12756
 
*.color7:  #a89984
